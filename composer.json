{
    "name": "prestamosedin/fullstack-app",
    "description": "Sistema de Gestión de Préstamos - Fullstack PHP + React",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "ext-pdo": "*",
        "ext-pgsql": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "PrestamosEdin\\": "backend/"
        }
    },
    "scripts": {
        "start": "php -S 0.0.0.0:8000 index.php"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.1"
        }
    }
}